<h1>Blogs#new</h1>
  <%= form_with(model: @blog, local: true) do |form| %>
    <div class="blog__title">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="blog__contents">
      <%= form.label :content %>
      <%= form.text_field :contentn %>
    </div>
    <div class="blog__new_submit">
      <%= form.submit %>
    </div>
  <% end %>
